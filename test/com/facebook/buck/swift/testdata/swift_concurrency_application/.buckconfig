[cache]
  mode = dir
[cxx]
  default_platform = iphonesimulator-x86_64
  cflags = -g
[apple]
  iphonesimulator_target_sdk_version = 13.0
  default_debug_info_format_for_binaries = NONE
  default_debug_info_format_for_libraries = NONE
  # TODO(T109413786): Remove once we are on Swift 5.6 (Xcode 13.3?)
  swift-stdlib-tool_replacement = swift-stdlib-tool-xcode132-fix
